> ## done:
> # 1) data loading
> # 2) data extraction
> # 3) data plot && save
> # 4) ADF tests for 3 series, based on AIC, BIC and significance criteria, in versions for constant and trend
> # 5) ADF tests for differences in 3 series, based on AIC, BIC  and siginficance criteria, in version for constant only
> ##
> 
> 
> library("uroot")
> 
> 
> #STEP 1 LOAD DATA
> set_data<-read.csv("/home/jinxs/documents/article_duty_impact/original.csv", header = TRUE, sep = ",", quote="\"")
> Date<-set_data[,1]
> 
> 
> #STEP 2 EXTRACT DATA FROM THE FILE AND MAKE THEM TIME SERIES
> crude<-set_data[,4]
> super_unleaded<-set_data[,5]
> premium_unleaded<-set_data[,6]
> derv<-set_data[,7]
> burining_oil<-set_data[,8]
> 
> tcrude<-ts(data=crude, start=c(1991,1), frequency=12)
> tsuper_unleaded<-ts(data=super_unleaded, start=c(1991,1), frequency=12)
> tpremium_unleaded<-ts(data=premium_unleaded, start=c(1991,1), frequency=12)
> tderv<-ts(data=derv, start=c(1991,1), frequency=12)
> 
> d_tcrude<-diff(tcrude)
> d_tsuper_unleaded<-diff(tsuper_unleaded)
> d_tpremium_unleaded<-diff(tpremium_unleaded)
> d_tderv<-diff(tderv)
> 
> 
> #STEP 3 PLOT && SAVE THE DATA 
> X11()
> plot.ts(cbind(tsuper_unleaded, tpremium_unleaded, tderv), col=c("blue", "red", "green"), plot.type="single", key, xy.lines="TRUE", y=NULL, ylab="Super unleaded", xlab=NULL)
> legend(x="topleft", cex=0.8, bty="n", lty=c(1,1,2), lwd=c(2,1,1), col=c("blue", "red", "green"), legend=c("Super unleaded", "Premium unleaded", "Derivative"))
> dev.print(postscript, horizontal = FALSE, onefile = FALSE, pointsize=10, file="/home/jinxs/documents/article_duty_impact/y_series.eps")
X11 
  2 
> 
> 
> #STEP 5 RUN THE STATS AND SAVE THE TABLES, formatting almost done, itsc, constant, trend, seasonal dummies
> 
> #############################################################################################################################################################################################################################################################################################################################################################STEP 5.1 ADF for LEVELS
> 
> #########################################################################################################################################################################################################################################SUPER UNLEADED
> 
> #STEP 5.1.1.A ADF, CONSTANT NO TREND
> 
> #STEP 5.1.1.1.A ADF, BIC, constant, no trend, no seasonality
> tsuper_unleaded_adfstat_c=ADF.test(wts=tsuper_unleaded, itsd=c(1,0,0), selectlags=list(mode="bic", Pmax=12))
> capture.output(urt.xtable(tsuper_unleaded_adfstat_c, label="tsuper_ADF_BIC_const", caption="ADF test statistcs for Super Unleaded Petrol (lags set to BIC, constant only)"), file="/home/jinxs/documents/article_duty_impact/tsuper_unleaded_adfstat_bic_c.tex")
NULL
> 
> 
> #STEP 5.1.1.2.a ADF, AIC, constant, no trend, no seasonality
> tsuper_unleaded_adfstat_c=ADF.test(wts=tsuper_unleaded, itsd=c(1,0,0), selectlags=list(mode="aic", Pmax=12))
> capture.output(urt.xtable(tsuper_unleaded_adfstat_c, label="tab:tsuper_ADF_AIC_const", caption="ADF test statistcs for Super Unleaded Petrol (lags set to AIC, constant only)"), file="/home/jinxs/documents/article_duty_impact/tsuper_unleaded_adfstat_aic_c.tex")
NULL
> 
> 
> #STEP 5.1.1.3.A ADF, SIGNF constant, no trend, no seasonality
> 
> tsuper_unleaded_adfstat_c=ADF.test(wts=tsuper_unleaded, itsd=c(1,0,0),  selectlags=list(mode="signf", Pmax=12))
> capture.output(urt.xtable(tsuper_unleaded_adfstat_c, label="tab:tsuper_ADF_SIGNF_const", caption="ADF test statistcs for Super Unleaded Petrol (significant lags, constant only)"), file="/home/jinxs/documents/article_duty_impact/tsuper_unleaded_adfstat_SIGNF_c.tex")
NULL
> 
> #####################################################################################################################STEP 5.1.2 ADF, CONSTANT, TREND
