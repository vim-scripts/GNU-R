> premium_unleaded<-set_data[,6]
> derv<-set_data[,7]
> burining_oil<-set_data[,8]
> 
> tcrude<-ts(data=crude, start=c(1991,1), frequency=12)
> tsuper_unleaded<-ts(data=super_unleaded, start=c(1991,1), frequency=12)
> tpremium_unleaded<-ts(data=premium_unleaded, start=c(1991,1), frequency=12)
> tderv<-ts(data=derv, start=c(1991,1), frequency=12)
> 
> d_tcrude<-diff(tcrude)
> d_tsuper_unleaded<-diff(tsuper_unleaded)
> d_tpremium_unleaded<-diff(tpremium_unleaded)
> d_tderv<-diff(tderv)
> 
> 
