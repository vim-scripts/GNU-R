> #STEP 1 LOAD DATA
> set_data<-read.csv("/home/jinxs/documents/article_duty_impact/original.csv", header = TRUE, sep = ",", quote="\"")
> Date<-set_data[,1]
> 
> 
> #STEP 2 EXTRACT DATA FROM THE FILE AND MAKE THEM TIME SERIES
> crude<-set_data[,4]
> super_unleaded<-set_data[,5]
> premium_unleaded<-set_data[,6]
> derv<-set_data[,7]
> burining_oil<-set_data[,8]
> 
> tcrude<-ts(data=crude, start=c(1991,1), frequency=12)
> tsuper_unleaded<-ts(data=super_unleaded, start=c(1991,1), frequency=12)
> tpremium_unleaded<-ts(data=premium_unleaded, start=c(1991,1), frequency=12)
> tderv<-ts(data=derv, start=c(1991,1), frequency=12)
> 
> d_tcrude<-diff(tcrude)
> d_tsuper_unleaded<-diff(tsuper_unleaded)
> d_tpremium_unleaded<-diff(tpremium_unleaded)
> d_tderv<-diff(tderv)
> 
> 
> #STEP 3 PLOT && SAVE THE DATA 
> X11()
> plot.ts(cbind(tsuper_unleaded, tpremium_unleaded, tderv), col=c("blue", "red", "green"), plot.type="single", key, xy.lines="TRUE", y=NULL, ylab="Super unleaded", xlab=NULL)
> legend(x="topleft", cex=0.8, bty="n", lty=c(1,1,2), lwd=c(2,1,1), col=c("blue", "red", "green"), legend=c("Super unleaded", "Premium unleaded", "Derivative"))
> dev.print(postscript, horizontal = FALSE, onefile = FALSE, pointsize=10, file="/home/jinxs/documents/article_duty_impact/y_series.eps")
